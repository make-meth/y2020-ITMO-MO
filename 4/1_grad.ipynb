    "import torch\n",
    "\n",
    "def grad(func, point):\n",
    "    x = torch.tensor(point, requires_grad=True)\n",
    "    y = func(*x)\n",
    "    y.backward()\n",
    "    return x.grad\n",
    "\n",
    "f = lambda *x: (9*x[0]**2 + 7*x[0]*x[1] + 5*x[1]**2 - 3*x[2] - 7*x[1]) ** 2\n",
    "print(grad(f, [10.0, 7.0, 5.0]).numpy())"
   ]
  }
